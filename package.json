{
  "name": "akwaba",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "concurrently \"npm run server\" \"npm run serve\"",
    "start:prod": "serve dist",
    "server": "pouchdb-server -d db",
    "lint-fix": "vue-cli-service lint --fix",
    "build": "vue-cli-service build --modern",
    "build:ci": "npm run build --report",
    "li:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint --ext .js,.vue .",
    "lint:all:markdownlint": "npm run lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "npm run lint:all:*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~1.2.19",
    "@fortawesome/free-solid-svg-icons": "~5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.19.0",
    "formeo": "^1.1.1",
    "lodash": "^4.17.11",
    "lodash.truncate": "^4.4.2",
    "nprogress": "^0.2.0",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-adapter-idb": "^7.0.0",
    "pouchdb-authentication": "^1.1.3",
    "register-service-worker": "^1.6.2",
    "rxdb": "^8.2.0",
    "rxjs": "^6.5.2",
    "serve": "^11.0.1",
    "vue": "^2.6.10",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "leveldown": "^5.1.0",
    "pouchdb-server": "^4.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  }
}
